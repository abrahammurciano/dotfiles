[init]
defaultBranch = main

[user]
{{- if has "work" .groups }}
email = abraham.murciano@mobileye.com
{{- else }}
email = abrahammurciano@gmail.com
{{- end }}
name = Abraham Murciano

[credential]
helper = cache --timeout=360000

[branch]
autosetuprebase = always
autosetupmerge = true

[push]
default = current
autoSetupRemote = true

[alias]
psh = push -u
pll = pull --rebase
co = checkout
st = status
br = branch
last = show HEAD
cm = commit -m
del = branch -d
pr = fetch --prune
fpsh = push --force-with-lease -u
cb = checkout -b